{
  "name": "dorker-cli",
  "version": "1.0.0",
  "description": "High-performance Google Dork parser CLI",
  "main": "dist/index.js",
  "bin": {
    "dorker": "./dist/index.js"
  },
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc && chmod +x dist/index.js",
    "start": "node dist/index.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src/**/*.ts",
    "clean": "rm -rf dist/"
  },
  "keywords": [
    "dorker",
    "google",
    "search",
    "parser",
    "cli"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "blessed": "^0.1.81",
    "blessed-contrib": "^4.11.0",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "conf": "^12.0.0",
    "execa": "^8.0.1",
    "fast-csv": "^5.0.1",
    "figlet": "^1.7.0",
    "bloom-filters": "^3.0.1",
    "inquirer": "^9.2.15",
    "lodash": "^4.17.21",
    "ms": "^2.1.3",
    "normalize-url": "^8.0.1",
    "ora": "^8.0.1",
    "p-queue": "^8.0.1",
    "pino": "^8.19.0",
    "psl": "^1.9.0",
    "tldts": "^6.1.16",
    "validator": "^13.11.0",
    "yaml": "^2.4.1"
  },
  "devDependencies": {
    "@types/blessed": "^0.1.25",
    "@types/figlet": "^1.5.8",
    "@types/inquirer": "^9.0.7",
    "@types/lodash": "^4.17.0",
    "@types/ms": "^0.7.34",
    "@types/node": "^20.11.30",
    "@types/psl": "^1.1.3",
    "@types/validator": "^13.11.9",
    "eslint": "^8.57.0",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "tsx": "^4.7.1",
    "typescript": "^5.4.3",
    "vitest": "^1.4.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
